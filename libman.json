{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "destination": "wwwroot/lib/jquery/",
      "library": "jquery@3.4.0"
    },
   
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.0-beta3",
      "destination": "wwwroot/lib/bootstrap/"
    },

    {
      "provider": "cdnjs",
      "library": "font-awesome@5.15.3",
      "destination": "wwwroot/lib/"
    },

    {
      "provider": "cdnjs",
      "library": "bootstrap-icons@1.4.1",
      "destination": "wwwroot/lib/"
    }

  ]
}
