@model MwProject.Core.ViewModels.ProjectViewModel

@{
    ViewBag.Title = Model.Heading;


    bool NewProject = (Model.Project.Id == 0);

    bool currentUserIsProjectManager =  String.Equals(Model.CurrentUser.Id,Model.Project.ProjectManagerId);


    bool canConfirmRequest = !Model.Project.IsRequestConfirmed
                   && Model.Project.IsTechnicalProportiesConfirmed
                   && Model.Project.IsQualityRequirementsConfirmed
                   && Model.Project.IsGeneralRequirementsConfirmed
                   && Model.Project.IsEstimatedSalesConfirmed
                   && Model.CurrentUser.CanConfirmRequest;


    bool canWithdrawRequestConfirmation = Model.Project.IsRequestConfirmed
                   && !Model.Project.IsConfirmed
                   && Model.CurrentUser.CanConfirmRequest;



    bool canConfirm = !Model.Project.IsConfirmed
                   && Model.Project.IsCalculationConfirmed
                   && Model.Project.IsEconomicRequirementsConfirmed
                   &&  Model.Project.IsRequestConfirmed           
                   && !Model.Project.IsAccepted
                   && (Model.CurrentUser.CanConfirmProject || currentUserIsProjectManager);




    bool canWithdrawConfirmation = Model.Project.IsConfirmed
                   && !Model.Project.IsAccepted
                   && (Model.CurrentUser.CanConfirmProject || currentUserIsProjectManager);


    bool canAccept = !Model.Project.IsAccepted
                      && Model.Project.IsConfirmed
                      && Model.CurrentUser.CanAcceptProject;

    bool canWithdrawAcceptation = Model.Project.IsAccepted
                               && Model.CurrentUser.CanAcceptProject;

    


}

<!-- Nagłówek -->
<div class="row">
    <div class="col-10">
        <h1 class="mb-3 mt-4 display-4">
            @Model.Heading <span class="badge bg-secondary">@Model.Project.PriorityOfProject</span>
        </h1>
    </div>

    <div class="col-2">
        <div class="mt-2 mb-4 text-muted">
            <img src="@Url.Content("~/images/single-document.webp")" style="width: 100px;" />
        </div>
    </div>
</div>




<!-- Przyciski -->
@if (Model.Project.Id != 0)
{
    <div class="row">
        <div class="col-12">

            <button type="button" class="btn btn-secondary mb-3"
                    onclick='window.location.href = "@Url.Action("Projects", "Project", new { projectId = Model.Project.Id })"'>
                Wstecz
            </button>


            <button id="printButton" class="btn btn-secondary mb-3">
                Drukuj
            </button>

            <!--
                       <button class="btn btn-secondary mb-3"
                               onclick="generatePdf()">
                           Generuj PDF
                       </button>
            <button class="btn btn-secondary mb-3" onclick="printProject()">
                           Drukuj
                       </button>
                       history.back();history.forward();location.reload();
                        -->


            @* potwierdzenie wniosku *@

            @if (canConfirmRequest)
            {
                <button class="btn btn-lg btn-success mb-3" onclick="confirmRequest()">
                    <strong>Potwierdź Wniosek</strong>
                </button>
            }

            @if (canWithdrawRequestConfirmation && Model.Project.ProjectManagerId is null)
            {
                <button class="btn btn-success mb-3" onclick="withdrawRequestConfirmation()">
                    Wycofaj Potwierdzenie Wniosku
                </button>
            }


            @* potwierdzenie projektu *@

            @if (canConfirm)
            {
                <button class="btn btn-lg btn-warning mb-3" onclick="confirmProject()">
                    <strong>Potwierdź Projekt</strong>
                </button>
            }

            @if (canWithdrawConfirmation)
            {
                <button class="btn btn-warning mb-3" onclick="withdrawProjectConfirmation()">
                    Wycofaj Potwierdzenie Projektu
                </button>
            }
          
            @* akceptacja szefa *@

            @if (canAccept)
            {
                <button class="btn btn-lg btn-danger mb-3" onclick="acceptProject()">
                    <strong>Akceptuj Projekt</strong>
                </button>
            }

            @if (canWithdrawAcceptation)
            {
                <button class="btn btn-danger mb-3" onclick="withdrawProjectAcceptance()">
                    Wycofaj Akceptacje
                </button>
            }

            

            

            @*
                <button class="btn btn-outline-success btn-sm"
                        onclick='window.location.href = "@Url.Action("ProjectPriority", "Project", new { id = Model.Project.Id })"'>
                    Prio
                </button>
            *@

        </div>
    </div>

}

