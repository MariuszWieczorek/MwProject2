<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MwProject</title>

    <!-- przed bootstrap bo nadpisuje style przycisków nie działa btn-lg btn-sm -->
    <link href="~/lib/eonasdan-bootstrap-datatimepicker/css/bootstrap-datetimepicker-standalone.css" rel="stylesheet" />

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datapicker/css/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datapicker/css/bootstrap-datepicker3.css" rel="stylesheet" />

    <link href="~/css/counter1.css" rel="stylesheet" />

    <link href="~/css/validate.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <link href="~/lib/starrating/css/star-rating.min.css" rel="stylesheet" />
    <link href="~/lib/starrating/themes/krajee-fas/theme.css" rel="stylesheet" />

</head>
<body>

    <header>

        <nav class="navbar navbar-expand-sm navbar-light bg-light sticky-top">
            <div class="container-fluid">

                <a class="navbar-brand"
                   asp-area=""
                   asp-controller="Project"
                   asp-action="Projects">
                   Projekty
                </a>

       
                <button class="navbar-toggler"
                        type="button" 
                        data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent"
                        aria-expanded="false"
                        aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                 
                        
 
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Słowniki
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">

                                <li>
                                    <a class="dropdown-item"
                                       asp-area=""
                                       asp-controller="Requirement"
                                       asp-action="Requirements"
                                       asp-route-type="1">informacje ekonomiczne</a>
                                </li>

                               

                            </ul>
                        </li>
                         

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Prywatność</a>
                        </li>

                    </ul>
                    <partial name="_LoginPartial" />
                </div>

            </div>
        </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">

            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - MwProject - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <script src="~/lib/moment/moment.js"></script>
    <script src="~/lib/bootstrap-datapicker/js/bootstrap-datepicker.js"></script>
    <script src="~/lib/eonasdan-bootstrap-datatimepicker/js/bootstrap-datetimepicker.min.js"></script>
    <script src="~/lib/font-awesome/js/all.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/starrating/js/star-rating.min.js"></script>
    <script src="~/lib/starrating/themes/krajee-fas/theme.js"></script>

    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
